{
  "bsv.bzl.invocation.enabled": false,
  "bsv.bzl.invocation.invokeWithBuildEventStreaming": false,
  "bsv.bzl.invocation.hideOutputPanelOnSuccess": false,
  "bsv.bzl.invocation.buildEventPublishAllActions": false,

  "editor.bracketPairColorization.enabled": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": true
  },
  "editor.codeLens": true,
  "editor.colorDecorators": true,
  "editor.fontSize": 13,
  "editor.fontFamily": "SF Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": false,
  "editor.formatOnPaste": true,
  "editor.inlineSuggest.enabled": true,
  "editor.largeFileOptimizations": false,
  "editor.minimap.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.maxColumn": 120,
  "editor.minimap.showSlider": "always",
  "editor.overviewRulerBorder": false,
  "editor.rulers": [80, 120],
  "editor.scrollBeyondLastLine": false,
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 100,

  "emmet.includeLanguages": {
    "wxml": "html"
  },

  "explorer.decorations.badges": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,

  "files.associations": {
    "Fastfile": "ruby",
    "Matchfile": "ruby",
    "Appfile": "ruby",
    "Deliverfile": "ruby",
    "Pluginfile": "ruby",
    "*.tulsigen": "json",
    "*.tulsiconf": "json",
    "*.cjson": "jsonc",
    "*.wxss": "css",
    "*.wxs": "javascript"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.vscode": true,
    "**/__pycache__": true,
    "**/.pytest_cache": true,
    "**/node_modules": true,
    "venv": true,
    "*.sublime-*": true,
    "env*": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/env/**": true,
    "**/venv/**": true,
    "env-*": true
  },
  "files.autoSave": "afterDelay",
  "files.trimFinalNewlines": true,

  "git.enabled": true,
  "git.autofetch": true,

  "git-graph.repository.commits.fetchAvatars": true,
  "git-graph.repository.fetchAndPrune": true,

  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": true,
    "markdown": false,
    "python": false
  },

  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.views.repositories.files.layout": "tree",
  "gitlens.views.commitDetails.files.layout": "tree",
  "gitlens.views.commits.pullRequests.showForBranches": false,
  "gitlens.views.commits.pullRequests.enabled": false,
  "gitlens.views.commits.showBranchComparison": false,
  "gitlens.views.showRelativeDateMarkers": false,
  "gitlens.views.worktrees.showBranchComparison": false,
  "gitlens.views.worktrees.pullRequests.showForBranches": false,
  "gitlens.views.worktrees.pullRequests.enabled": false,

  "java.semanticHighlighting.enabled": true,
  "java.project.importHint": false,

  "prettier.jsxBracketSameLine": true,
  "prettier.jsxSingleQuote": true,

  "python.analysis.completeFunctionParens": true,
  "python.analysis.diagnosticMode": "workspace",
  "python.analysis.typeCheckingMode": "basic",
  "python.autoComplete.extraPaths": [],
  "python.analysis.extraPaths": [],
  "python.diagnostics.sourceMapsEnabled": true,
  "python.linting.pylintEnabled": true,
  "python.linting.enabled": true,
  "python.linting.lintOnSave": true,
  "python.languageServer": "Default",
  "python.terminal.activateEnvInCurrentTerminal": true,

  "react-native-tools.networkInspector.consoleLogsColorTheme": "Dark",

  "redhat.telemetry.enabled": true,

  "remote.SSH.configFile": "~/.ssh/config.local",

  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/env": true,
    "**/venv": true
  },

  "telemetry.telemetryLevel": "off",

  "terminal.integrated.fontFamily": "SF Mono, Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.lineHeight": 1.5,
  "terminal.integrated.cursorStyle": "block",
  "terminal.integrated.cursorBlinking": true,

  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",

  "window.closeWhenEmpty": false,
  "window.commandCenter": true,
  "window.title": "${activeEditorShort}${separator}${rootName}",
  "window.titleBarStyle": "custom",

  "workbench.colorCustomizations": {
    "[Material Theme]": {
      "titleBar.activeBackground": "#141c20",
      "titleBar.activeForeground": "#83a1af",
      "titleBar.border": "#141c20",
      "editorGroupHeader.tabsBackground": "#192227",
      "editorGroupHeader.noTabsBackground": "#192227",
      "tab.border": "#141c20",
      "tab.activeBorder": "#263238",
      "tab.unfocusedActiveBorder": "#263238",
      "tab.activeBackground": "#263238",
      "tab.inactiveBackground": "#192227",
      "tab.hoverBackground": "#21292e",
      "statusBar.background": "#0078cf",
      "statusBar.foreground": "#ffffff",
      "statusBar.border": "#0078cf",
      "statusBar.noFolderBackground": "#6a1f7c",
      "statusBar.noFolderForeground": "#ffffff",
      "statusBar.noFolderBorder": "#6a1f7c",
      "statusBar.debuggingBackground": "#ce662a",
      "statusBar.debuggingForeground": "#ffffff",
      "statusBar.debuggingBorder": "#ce662a",
      "sideBar.border": "#141c20",
      "sideBar.background": "#1e2a30",
      "panelTitle.activeBorder": "#80cbc4",
      "panel.border": "#141c20",
      "panel.background": "#192227",
      "editorOverviewRuler.border": "#141c20"
    },
    "[Material Theme Darker High Contrast]": {
      "statusBar.background": "#0078cf",
      "statusBar.foreground": "#ffffff",
      "statusBar.border": "#0078cf",
      "statusBar.noFolderBackground": "#6a1f7c",
      "statusBar.noFolderForeground": "#ffffff",
      "statusBar.noFolderBorder": "#6a1f7c",
      "statusBar.debuggingBackground": "#ce662a",
      "statusBar.debuggingForeground": "#ffffff",
      "statusBar.debuggingBorder": "#ce662a",
      "activityBarBadge.background": "#64FFDA",
      "activityBar.activeBorder": "#64FFDA",
      "list.activeSelectionForeground": "#64FFDA",
      "list.inactiveSelectionForeground": "#64FFDA",
      "list.highlightForeground": "#64FFDA",
      "scrollbarSlider.activeBackground": "#64FFDA50",
      "editorSuggestWidget.highlightForeground": "#64FFDA",
      "textLink.foreground": "#64FFDA",
      "progressBar.background": "#64FFDA",
      "pickerGroup.foreground": "#64FFDA",
      "tab.activeBorder": "#64FFDA",
      "notificationLink.foreground": "#64FFDA",
      "editorWidget.resizeBorder": "#64FFDA",
      "editorWidget.border": "#64FFDA",
      "settings.modifiedItemIndicator": "#64FFDA",
      "settings.headerForeground": "#64FFDA",
      "panelTitle.activeBorder": "#64FFDA",
      "breadcrumb.activeSelectionForeground": "#64FFDA",
      "menu.selectionForeground": "#8fdfcc",
      "menubar.selectionForeground": "#64FFDA",
      "editor.findMatchBorder": "#64FFDA",
      "selection.background": "#64FFDA40",
      "statusBarItem.remoteBackground": "#64FFDA"
    },
    "[Gruvbox Dark Hard]": {
      "activityBar.background": "#141617",
      "activityBar.border": "#101010",
      "badge.background": "#101010",
      "badge.foreground": "#a89984",
      "editorGroup.border": "#101010",
      "editorGroupHeader.tabsBackground": "#151819",
      "editorGroupHeader.tabsBorder": "#10101000",
      "editorGroupHeader.noTabsBackground": "#151819",
      "focusBorder": "#00000000",
      "listFilterWidget.background": "#1d2021",
      "listFilterWidget.outline": "#3c3836",
      "gitDecoration.ignoredResourceForeground": "#504945",
      "panel.border": "#101010",
      "panel.background": "#151819",
      "sideBar.border": "#101010",
      "sideBar.background": "#151819",
      "sideBar.foreground": "#928374",
      "sideBarSectionHeader.background": "#141617",
      "sideBarSectionHeader.border": "#101010",
      "statusBar.background": "#076678",
      "statusBar.foreground": "#ffffff",
      "statusBar.border": "#07667800",
      "statusBar.noFolderBackground": "#8f3f71",
      "statusBar.noFolderForeground": "#ffffff",
      "statusBar.noFolderBorder": "#8f3f7100",
      "statusBar.debuggingBackground": "#af3a03",
      "statusBar.debuggingForeground": "#ffffff",
      "statusBar.debuggingBorder": "#af3a0300",
      "tab.activeBackground": "#1d2021",
      "tab.activeBorder": "#00000000",
      "tab.border": "#00000000",
      "tab.hoverBackground": "#1d2021",
      "tab.inactiveBackground": "#151819",
      "terminal.background": "#151819",
      "titleBar.activeBackground": "#141617",
      "titleBar.inactiveBackground": "#141617",
      "titleBar.border": "#101010",
      "widget.shadow": "#1a1a1a"
    }
  },
  "workbench.colorTheme": "Gruvbox Dark Hard",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.tabSizing": "fit",
  "workbench.productIconTheme": "fluent-icons",
  "workbench.settings.editor": "json",
  "workbench.settings.openDefaultSettings": true,
  "workbench.settings.openDefaultKeybindings": true,
  "workbench.sideBar.location": "left",
  "workbench.startupEditor": "readme",
  "workbench.view.alwaysShowHeaderActions": false,

  "[bazel]": {
    "editor.defaultFormatter": "StackBuild.bazel-stack-vscode"
  },

  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },

  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "[python]": {
    "editor.tabSize": 4, // PEP 8 implementations. Read more: https://www.python.org/dev/peps/pep-0008/
    "editor.wordWrapColumn": 79
  },

  "[swift]": {
    "editor.tabSize": 4
  }
}
